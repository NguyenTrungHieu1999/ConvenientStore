                @*Cart area*@
                @{Html.RenderAction("Cart", "Home");}
                @*<div class="cart">
                        <span class="fa fa-shopping-cart my-cart-icon"><span class="badge badge-notify my-cart-badge"></span></span>
                    </div>*@
                <div class="search-box">
                    <form action="~/tim-kiem" method="get">
                        <input type="text" id="txtKeyword" name="keyword" placeholder="Tìm sản phẩm">
                        <input style="position:absolute;" type="submit" value="Tìm kiếm" class="btn-success">
                    </form>
                </div>
                <div class="clearfix"></div>
            </div>

        </div>
    </div>
    <!--Body of Homepage-->
    @RenderBody()
    <!--footer-->
    <div class="footer">
        <div class="container">
            <div class="footer-bottom">
                <h2><a href="/">Convinient Store<span>The Best Supermarket</span></a></h2>
                <p class="fo-para">Môn học bảo mật web</p>
                <ul class="social-fo">
                    <li><a href="#" class=" face"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                    <li><a href="#" class=" twi"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
